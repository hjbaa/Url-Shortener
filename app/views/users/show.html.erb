<% provide :page_title, "Your URLs" %>

<h1 class="mb-4">Your URLs</h1>

<table class="table table-striped table-hover">
  <thead>
  <tr>
    <th scope="col">#</th>
    <th scope="col">Short url</th>
    <th scope="col">Full URL</th>
    <th scope="col">Created at</th>
  </tr>
  </thead>
  <tbody>
  <%= render @urls %>
  </tbody>
</table>
